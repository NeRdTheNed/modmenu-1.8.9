  "contact": {
    "homepage": "https://minecraft.curseforge.com/projects/modmenu",
    "sources": "https://github.com/Prospector/ModMenu",
    "issues": "https://github.com/Prospector/ModMenu/issues"
  },
  "depends": {
    "fabricloader": ">=0.6.1",
    "minecraft": "1.8.x"
  },
  "authors": [
    "Prospector"
  ],
  "contributors": [
    "shedaniel",
    "Draylar",
    "Mordna",
    "Madis0",
    "Yanis48",
    "Boundarybreaker",
    "fewizz",
    "XuyuEre",
    "geniiii",
    "thiagokenis",
    "Juuxel",
    "InsomniaKitten",
    "modmuss50",
    "Patbox",
    "Lykrast",
    "Pyrofab",
    "UpcraftLP",
    "swordglowsblue",
    "hinataaki",
    "LeonXu98",
    "magneticflux"
  ],
  "description": "Adds a mod menu to view the list of mods you have installed.",
  "accessWidener": "modmenu.aw",
  "mixins": [
    "mixins.modmenu.json"
  ],
  "custom": {
    "modmenu:clientsideOnly": true
  }
}
