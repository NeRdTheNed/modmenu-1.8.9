{
  "schemaVersion": 1,
  "id": "modmenu",
  "name": "Mod Menu",
  "version": "$version",
  "environment": "client",
  "license": "MIT",
  "icon": "assets/modmenu/icon.png",
  "entrypoints": {
    "client": [
      "io.github.prospector.modmenu.ModMenu"
    ],
    "modmenu": [
      "io.github.prospector.modmenu.VanillaModMenuCompat"
    ]
